<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.AdminDao">
	<select id="findoneadmin" resultType="Admin">
		select * from admins where name=#{name} and pwd=#{pwd}
	</select>
</mapper>